<h2 class="page-header">Blogs</h2>
<div class="blogs-container">
  <% blogs.forEach(blog => { %>
  <article class="blog">
    <div class="blog-header">
      <h3 class="blog-title"><a href="/blogs/<%= blog.id %>/<%= blog.stringId %>"><%= blog.title %></a></h3>
      <div class="blog-date"><time><%= blog.getCreatedAtDate %></time></div>
    </div>
    <p class="blog-description"><%= blog.description %></p>
  </article>
  <% }) %>
</div>
<%- include('../partials/errorMessage') %>